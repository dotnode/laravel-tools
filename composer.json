{
    "name": "luffyzhao/laravel-tools",
    "require": {
        "php": "^7.0",
        "illuminate/auth": ">=5.5",
        "illuminate/contracts": ">=5.5",
        "illuminate/http": ">=5.5",
        "illuminate/support": ">=5.5",
        "illuminate/redis": ">=5.5",
        "illuminate/console": ">=5.5",
        "illuminate/database": ">=5.5",
        "illuminate/routing": ">=5.5",
        "mockery/mockery": ">=0.9.9",
        "lcobucci/jwt": "^3.2",
        "namshi/jose": "^7.0",
        "nesbot/carbon": "1.* | 2.*",
        "maatwebsite/excel": "3.*",
        "ext-openssl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8 || ~6.0"
    },
    "autoload": {
        "psr-4": {
            "LTools\\": "src/"
        }
    },
    "authors": [
        {
            "name": "luffyzhao",
            "email": "luffyzhao@vip.126.com"
        }
    ],
	"extra": {
        "laravel": {
            "providers": [
                "LTools\\Providers\\LaravelServiceProvider"
            ]
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
